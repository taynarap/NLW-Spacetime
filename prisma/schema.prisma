// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id   String @id @default(uuid()) //ISTO SIGNIFICA QUE O ID EH UMA CHAVE PRIMARIA. UNIQUE UNIVERSAL ID -UUID
  nome String
}
